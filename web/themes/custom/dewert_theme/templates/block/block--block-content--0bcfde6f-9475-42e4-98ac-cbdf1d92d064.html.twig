{#
/**
 * @file
 * Block Histoire de famille (expertise)
 */
#}
{%
  set classes = [
    'section-story',
    'spaced',
  ]
%}
<section{{ attributes.addClass(classes)|without('id') }}>
  <div class="section-story__content">
  {{ title_prefix }}
  {% if label %}
    <h1 {{ title_attributes.addClass('section-story__title') }}>{{ label }}</h1>
  {% endif %}
  {{ title_suffix }}
  {% block content %}
    <div class="section-story__images">
      <div class="section-story__image_01"></div>
      <div class="section-story__image_02"></div>
      <div class="section-story__image_03"></div>
    </div>
    <div class="section-story__text">
    {{ content }}
    </div>
  {% endblock %}
  </div>
</section>
